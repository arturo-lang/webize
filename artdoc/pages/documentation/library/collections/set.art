name: "set"
address: "0x109EAA7C8"
type: :function
module: "Collections"
returns: [
        :nothing
    ]
description: "set collection\'s item at index to given value"
example: {
        myDict: #[ 
            name: "John"
            age: 34
        ]
        
        set myDict 'name "Michael"        ; => [name: "Michael", age: 34]
        
        arr: [1 2 3 4]
        set arr 0 "one"                   ; => ["one" 2 3 4]
    }
attributes: #[
    ]
arguments: #[
    collection: [
            :string
             :dictionary
             :block
        ]
    index: [
            :any
        ]
    value: [
            :any
        ]
    ]
related: [
