name: "strip"
address: "0x109EF29A8"
type: :function
module: "Strings"
returns: [
        :string
         :nothing
    ]
description: "strip whitespace from given string"
example: {
        str: "     Hello World     "
        
        print ["strip all:"      ">" strip str       "<"]
        print ["strip leading:"  ">" strip.start str "<"]
        print ["strip trailing:" ">" strip.end str   "<"]
        
